<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:macros="sap.fe.macros"
>
    <VBox>
        <!-- <Button
            id="_IDGenButton1"
            core:require="{ handler: 'merge/ext/fragment/College_Details'}"
            text="College_Details"
            press="handler.onPress"
        /> -->

        <Table 
			headerText="College Details"
			items="{
			path:'/college'
		}"
		>
            <columns>
                <Column mergeDuplicates="true">
                    <header>
                        <Text text="College ID" />
                    </header>
                </Column>
                <Column mergeDuplicates="true">
                    <header>
                        <Text text="College Name" />
                    </header>
                </Column>
                <Column mergeDuplicates="true">
                    <header>
                        <Text text="College Type" />
                    </header>
                </Column>
                <Column mergeDuplicates="true">
                    <header>
                        <Text text="College Address" />
                    </header>
                </Column>
            </columns>
			<ColumnListItem vAlign="Middle">
			<Text text="{collegeId}" />
			<Text text="{collegeName}" />
			<Text text="{collegeType}" />
			<Text text="{collegeAddress}" />
			</ColumnListItem>
        </Table>
    </VBox>
</core:FragmentDefinition>




	
